<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jfsb.antwortn">

    <application
        android:allowBackup="true"
        android:icon="@drawable/antwort_iso"
        android:label="@string/app_name"
        android:roundIcon="@drawable/antwort_iso"
        android:supportsRtl="true"
        android:theme="@style/Theme.AntwortN"
        android:usesCleartextTraffic="true">
        <activity android:name=".activities.UserActivity"></activity>
        <activity android:name=".activities.MainActivity" />
        <activity android:name=".post.CreateActivity"/>
        <activity android:name=".activities.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>